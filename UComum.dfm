  object IBAReceber: TIBOQuery
    Params = <
      item
        DataType = ftInteger
        Name = 'CD_CLIENTE'
        ParamType = ptInput
      end>
    ColumnAttributes.Strings = (
      'SEQUENCIAL=REQUIRED')
    DatabaseName = 'F:\Bases de clientes\Base NOVA\FOCUS.FOC'
    DeleteSQL.Strings = (
      'DELETE FROM CONTASARECEBER'
      'WHERE'
      '   SEQUENCIAL = :OLD_SEQUENCIAL')
    EditSQL.Strings = (
      'UPDATE CONTASARECEBER SET'
      '   SEQUENCIAL = :SEQUENCIAL, /*PK*/'
      '   NF_NUMERO = :NF_NUMERO,'
      '   CD_CLIENTE = :CD_CLIENTE,'
      '   VALORATUAL = :VALORATUAL,'
      '   DT_EMISSAO = :DT_EMISSAO,'
      '   NUM_TERMINAL = :NUM_TERMINAL,'
      '   LOGIN = :LOGIN,'
      '   VENCIMENTOORIGINAL = :VENCIMENTOORIGINAL,'
      '   NUM_BANCO = :NUM_BANCO,'
      '   AGENCIA = :AGENCIA,'
      '   NUM_CONTA = :NUM_CONTA,'
      '   NUM_CHEQUE = :NUM_CHEQUE,'
      '   TIPO_PAGAMENTO = :TIPO_PAGAMENTO,'
      '   VENDEDOR = :VENDEDOR,'
      '   BAIXAR = :BAIXAR,'
      '   ABATIDO = :ABATIDO,'
      '   VALORORIGINAL = :VALORORIGINAL,'
      '   VENCIMENTOATUAL = :VENCIMENTOATUAL,'
      '   BOL_NOSSONUMERO = :BOL_NOSSONUMERO,'
      '   IMPRIMIU = :IMPRIMIU,'
      '   IMPRIMIR = :IMPRIMIR,'
      '   NUM_DOC_VENDA = :NUM_DOC_VENDA,'
      '   NUM_PEDIDO = :NUM_PEDIDO,'
      '   NUM_DOC_CONTA = :NUM_DOC_CONTA,'
      '   OP_NUM_TERMINAL = :OP_NUM_TERMINAL,'
      '   CH_STATUS = :CH_STATUS,'
      '   CH_DT_STATUS = :CH_DT_STATUS,'
      '   CH_REPASSADO_PARA = :CH_REPASSADO_PARA,'
      '   CH_CD_CONTA_CLIENTE = :CH_CD_CONTA_CLIENTE,'
      '   CH_DEP_NUM_CONTA = :CH_DEP_NUM_CONTA,'
      '   PARCELA = :PARCELA,'
      '   OBS = :OBS,'
      '   INATIVO = :INATIVO'
      'WHERE'
      '   SEQUENCIAL = :OLD_SEQUENCIAL')
    IB_Connection = DtM.IBDatabaseServer
    IB_Transaction = TrAReceber
    InsertSQL.Strings = (
      'INSERT INTO CONTASARECEBER('
      '   SEQUENCIAL, /*PK*/'
      '   NF_NUMERO,'
      '   CD_CLIENTE,'
      '   VALORATUAL,'
      '   DT_EMISSAO,'
      '   NUM_TERMINAL,'
      '   LOGIN,'
      '   VENCIMENTOORIGINAL,'
      '   NUM_BANCO,'
      '   AGENCIA,'
      '   NUM_CONTA,'
      '   NUM_CHEQUE,'
      '   TIPO_PAGAMENTO,'
      '   VENDEDOR,'
      '   BAIXAR,'
      '   ABATIDO,'
      '   VALORORIGINAL,'
      '   VENCIMENTOATUAL,'
      '   BOL_NOSSONUMERO,'
      '   IMPRIMIU,'
      '   IMPRIMIR,'
      '   NUM_DOC_VENDA,'
      '   NUM_PEDIDO,'
      '   NUM_DOC_CONTA,'
      '   OP_NUM_TERMINAL,'
      '   CH_STATUS,'
      '   CH_DT_STATUS,'
      '   CH_REPASSADO_PARA,'
      '   CH_CD_CONTA_CLIENTE,'
      '   CH_DEP_NUM_CONTA,'
      '   PARCELA,'
      '   OBS,'
      '   INATIVO)'
      'VALUES ('
      '   :SEQUENCIAL,'
      '   :NF_NUMERO,'
      '   :CD_CLIENTE,'
      '   :VALORATUAL,'
      '   :DT_EMISSAO,'
      '   :NUM_TERMINAL,'
      '   :LOGIN,'
      '   :VENCIMENTOORIGINAL,'
      '   :NUM_BANCO,'
      '   :AGENCIA,'
      '   :NUM_CONTA,'
      '   :NUM_CHEQUE,'
      '   :TIPO_PAGAMENTO,'
      '   :VENDEDOR,'
      '   :BAIXAR,'
      '   :ABATIDO,'
      '   :VALORORIGINAL,'
      '   :VENCIMENTOATUAL,'
      '   :BOL_NOSSONUMERO,'
      '   :IMPRIMIU,'
      '   :IMPRIMIR,'
      '   :NUM_DOC_VENDA,'
      '   :NUM_PEDIDO,'
      '   :NUM_DOC_CONTA,'
      '   :OP_NUM_TERMINAL,'
      '   :CH_STATUS,'
      '   :CH_DT_STATUS,'
      '   :CH_REPASSADO_PARA,'
      '   :CH_CD_CONTA_CLIENTE,'
      '   :CH_DEP_NUM_CONTA,'
      '   :PARCELA,'
      '   :OBS,'
      '   :INATIVO)')
    RecordCountAccurate = True
    SQL.Strings = (
      'SELECT                                                      '
      'R.*,                                                        '
      'C.NM_CLIENTE,                                               '
      'R.NUM_DOC_CONTA||coalesce(R.PARCELA, '#39#39') as NUM_DOCUMENTO,'
      'A.ATRASO,                                                   '
      'coalesce(J.JUROS, 0) as JUROS,                              '
      'coalesce( R.ATRASO_MULTA , 0) as CONTA_ATRASO_MULTA,        '
      
        'coalesce( R.ATRASO_JUROSAODIA , 0) as CONTA_ATRASO_JUROSAODIA,  ' +
        ' '
      '(R.VALORATUAL + (coalesce(J.JUROS, 0))) as SUBTOTAL,        '
      'P.DESCRICAO as DESC_TIPO_PAGAMENTO,                         '
      
        'RC.BLOQUEADO, RC.BLOQUEIOMANUAL, RC.MAIORATRASO, RC.SALDODADIVID' +
        'A,'
      'U.NM_USUARIO as NOMEVENDEDOR                                '
      
        'from V_CONTASARECEBER R                                         ' +
        '    '
      'left outer join CLIENTES C on (R.CD_CLIENTE = C.CD_CLIENTE) '
      
        'left outer join REFCLIENTES RC on (R.CD_CLIENTE = RC.CD_CLIENTE)' +
        ' '
      
        'left outer join SP_CALC_ATRASO(R.VENCIMENTOATUAL, current_date, ' +
        'R.CALCULAMULTAJUROS) A on 1=1'
      
        'left outer join SP_JUROSDACONTA(R.VALORATUAL, R.VENCIMENTOATUAL,' +
        ' current_date, coalesce(R.ABATIDO, 0), R.CALCULAMULTAJUROS, 1,  ' +
        'R.ATRASO_MULTA ,  R.ATRASO_JUROSAODIA , A.ATRASO ) J on 1=1'
      'left outer join SP_TIPOPAGAMENTO(R.TIPO_PAGAMENTO) P on 1=1 '
      'left outer join USUARIOS U on U.CD_USUARIO = R.VENDEDOR     '
      'where coalesce(R.INATIVO, '#39'F'#39') <> '#39'V'#39'                   '
      'and coalesce(R.BAIXADO, '#39'F'#39') <> '#39'V'#39
      'and C.CD_CLIENTE = :CD_CLIENTE '
      ''
      'order by VENCIMENTOATUAL, SEQUENCIAL')
    FieldOptions = []
    Left = 232
    Top = 265
